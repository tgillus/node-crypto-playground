  "dependencies": {
    "jose": "5.6.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@types/node": "20.14.13",
    "@vitest/coverage-v8": "2.0.4",
    "del-cli": "5.1.0",
    "husky": "9.1.4",
    "lint-staged": "15.2.7",
    "tsx": "4.16.2",
    "typescript": "5.5.4",
    "vitest": "2.0.4"
  },
  "license": "ISC",
  "name": "node-crypto-playground",
  "scripts": {
    "build": "npm run clean && npm test && tsc",
